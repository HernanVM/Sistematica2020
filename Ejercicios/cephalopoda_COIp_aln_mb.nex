#NEXUS
begin data;
	dimensions ntax=7 nchar=657;
	format datatype=dna missing=? gap=-;
matrix
Nautilus       -ATTTTTCGATCCAGCTGGAGGAGGAGATCCAATTCTTTATCAACACTTATT
Argonauta      -TTTTTTTGACCCTAGAGGTGGAGGAGACCCTATTTTATATCAACATTTATT
Opisthoteuthis 
Vampyroteuthis -CTTTTTTGATCCAAGAGGTGGAGGAGACCCTATCTTATACCAACATTTATT
Sepia          -ATTTTTTGATCCTAGAGGTGGTGGAGATCCAATCTTATATCAACATTTATT
Dosidicus      
Octopus        -TTTTTTTGATCCGAGAGGGGGGGGAGACCCAATTTTATATCAACACTTATT
;
end;

BEGIN mrbayes;

[ start log file and replace the existing one ]
log start filename=coi_ceph.log replace;

[ setting the outgroup and sets ]
outgroup Nautilus;
charset COI = 1-657;
partition by_gene = 1: COI;
set partition=by_gene;

[ model settings and unlink model parameters ]
lset applyto=(all) nst=6 rates=invgamma;
unlink shape=(all);

prset applyto=(1)
revmatpr=fixed(0.0100,10.4168,0.0100,3.8538,37.1448,1.0000)
statefreqpr=fixed(0.2631,0.1665,0.2120,0.3583)
pinvarpr=fixed(0.3460)
shapepr=exponential(0.7640);

mcmcp
nchains=4
ngen=1000000
samplefreq=100
savebrlens=yes
printfreq=1000;

end;
